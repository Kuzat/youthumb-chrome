{
	"manifest_version": 2,
	"name": "Youtube Thumbnail Finder",
	"short_name": "Youthumb",
	"description": "Add button on youtube videos to easly get thumbnail",
	"version": "1.3.0",
	"author": "Vegard Bj√∏rsvik",
	"homepage_url": "https://youthumb.net",
	
	"icons": {
		"128": "logo.png"
	},
	
	"permissions": [
		"*://www.youtube.com/*"
	],
	
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["button.js"],
			"run_at": "document_end"
		}
	]
}